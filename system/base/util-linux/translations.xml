<?xml version="1.0" ?>
<INARY>
    <Source>
        <Name>util-linux</Name>
        <Summary xml:lang="tr">Çeşitli kullanışlı Linux araçları</Summary>
        <Summary xml:lang="fr">Divers utilitaires Linux utiles.</Summary>
        <Description xml:lang="tr">util-linux paketi, bir Linux sisteminin işlevsel olması için gerekli olan birçok düşük seviye sistem uygulamasını içerir.</Description>
    </Source>

    <Package>
        <Name>libutil-linux</Name>
        <Summary xml:lang="tr">util-linux için kitaplıklar</Summary>
    </Package>

    <Package>
        <Name>libutil-linux-devel</Name>
        <Summary xml:lang="tr">util-linux kitaplıkları için başlık dosyaları</Summary>
    </Package>

    <Package>
        <Name>uuidd</Name>
        <Summary xml:lang="tr">Zaman tabanlı UUID'lerin benzersizliğini garantileyen sistem hizmeti</Summary>
    </Package>

    <Package>
        <Name>libutil-linux-32bit</Name>
        <Summary xml:lang="tr">util-linux için 32-bit paylaşımlı kitaplıklar</Summary>
    </Package>
    <Package>
        <Name>util-linux-docs</Name>
        <Summary xml:lang="tr">util-linux için belgelendirme dosyaları</Summary>
