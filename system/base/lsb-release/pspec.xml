            <AdditionalFile owner="root" permission="0644" target="/etc/lsb-release">lsb-release</AdditionalFile>
        </AdditionalFiles>
    </Package>
    <Package>
        <Name>lsb-release-docs</Name>
        <IsA>data:doc</IsA>
        <PartOf>system.doc</PartOf>
