    <Package>
      	<Name>acl-pages</Name>
	      <IsA>data:doc</IsA>
        <PartOf>system.doc</PartOf>
	      <Summary>Man pages for acl package.</Summary>
        <RuntimeDependencies>
            <Dependency release="current">acl</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="man">/usr/share/man/</Path>
        </Files>
    </Package>

