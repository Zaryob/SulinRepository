            <Dependency>gc-devel</Dependency>
            <Dependency>gmp-devel</Dependency>
            <Dependency>guile-devel</Dependency>
            <Dependency>libxml2-devel</Dependency>
            <Dependency>which</Dependency>
        </BuildDependencies>
